{
    "name": "Quick Dark Mode",
    "description": "Quickly add a dark mode to a site that doesnt have it",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "commands": {
        "darkmode": {
            "suggested_key": {
                "default": "Shift+Alt+D"
            },
            "description": "Add darkmode"
        }
    }
}
